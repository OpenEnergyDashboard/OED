    "morgan": "~1.9.0",
    "multer": "~1.3.1",
    "ngraph.graph": "~20.0.0",
    "ngraph.path": "~1.4.0",
    "nodemailer": "~6.6.1",
    "pg": "~8.10.0",
    "pg-promise": "~11.5.0",
    "plotly.js": "~2.18.1",
    "prop-types": "~15.8.1",
    "query-string": "~7.0.1",
