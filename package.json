    "css-loader": "~6.8.1",
    "eslint": "~8.48.0",
    "eslint-plugin-jsdoc": "~46.5.0",
    "lodash-webpack-plugin": "~0.11.6",
    "mocha": "~9.2.2",
    "node-polyfill-webpack-plugin": "~1.1.4",
    "nodemon": "~2.0.22",
    "sinon": "~12.0.0",
    "source-map-loader": "~4.0.1",
    "stream-browserify": "~3.0.0",
    "style-loader": "~3.3.1",
    "terser-webpack-plugin": "~5.3.9",
    "ts-loader": "~9.2.0",
    "typescript": "~5.1.6",
    "webpack": "~5.76.0",
    "webpack-cli": "~5.1.4"
  }
}
